record node
    data : integer
    next : refersTo node

define start()
variables head, n : refersTo node
    allocateMemory var head
    allocateMemory var n
    head.data := 5
    head.next := n
    head.next.data := 3
    write head.data, head.next.data
    freeMemory var n
    freeMemory var head
