define start()
constants start = 1, innerLoopLen = 44, outerLoopLen = 70000000
variables i, j, prev1, prev2, curr : integer
    for j from start to outerLoopLen
        prev1 := 1
        prev2 := 1
        curr := 1
        for i from start to innerLoopLen
            curr := prev1 + prev2
            prev2 := prev1
            prev1 := curr
    write curr
